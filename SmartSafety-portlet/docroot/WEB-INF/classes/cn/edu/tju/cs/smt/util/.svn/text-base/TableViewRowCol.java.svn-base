package cn.edu.tju.cs.smt.util;

public class TableViewRowCol {
	public String rowName;
	public boolean canMapView;
	public boolean canLineView;
	public boolean canPieView;
	public boolean canHistogramView;
	/**
	 * 
	 * @param name 列名
	 * @param map 地图可视化
	 * @param line 折线图
	 * @param pie 饼图
	 * @param bar 柱状图
	 */
	public TableViewRowCol(String name, boolean map, boolean line, boolean pie, boolean bar) {
		this.rowName = name;
		this.canMapView = map;
		this.canPieView = pie;
		this.canLineView = line;
		this.canHistogramView = bar;
	}
}
